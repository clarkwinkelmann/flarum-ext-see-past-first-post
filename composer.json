{
    "name": "clarkwinkelmann/flarum-ext-see-past-first-post",
    "description": "Permission to let users see past the first post of discussions",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": ">=0.1.0-beta.15 <0.1.0-beta.16"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "homepage": "https://clarkwinkelmann.com/",
            "email": "clark.winkelmann@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/clarkwinkelmann/flarum-ext-see-past-first-post",
        "forum": "https://discuss.flarum.org/d/23077"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\SeePastFirstPost\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "See past first post",
            "category": "discussion",
            "icon": {
                "name": "fas fa-stream",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/23077"
        }
    }
}
